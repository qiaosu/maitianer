<h1>Listing photos</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Image</th>
    <th>Description</th>
    <th>Privacy strategy</th>
    <th>Status</th>
    <th>Timeline</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @photos.each do |photo| %>
  <tr>
    <td><%= photo.title %></td>
    <td><%= photo.image %></td>
    <td><%= photo.description %></td>
    <td><%= photo.privacy_strategy %></td>
    <td><%= photo.status %></td>
    <td><%= photo.timeline %></td>
    <td><%= link_to 'Show', photo %></td>
    <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
    <td><%= link_to 'Destroy', photo, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Photo', new_photo_path %>
