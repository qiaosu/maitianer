<% @title = "自定义时间轴" %>

<%= form_for [@baby, @baby.timeline] do |f| %>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :status %><br />
    <%= f.number_field :status %>
  </p>
  <p><%= f.submit %></p>
<% end %>


